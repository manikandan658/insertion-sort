#include<stdio.h>
int main() {
    int i,j,a[10],temp,n=7;
    printf("enterthe array element:");
    for(i=0;i<n;i++)
    scanf("%d",&a[i]);
    for(i=1;i<n;i++)
    {
        temp=a[i];
        j=i-1;
        while(j>=0&&a[j]>temp)
        {
            a[j+1]=a[j];
            j--;
        }
        a[j+1]=temp;
    }
    printf("the sorted array elements are:");
    for(i=0;i<n;i++)
    printf("%d  ",a[i]);
}
